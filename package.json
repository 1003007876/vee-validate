{
  "name": "vee-validate",
  "version": "3.0.0-beta.0",
  "description": "Template Based Validation Framework for Vue.js",
  "author": "Abdelrahman Awad <logaretm1@gmail.com>",
  "license": "MIT",
  "module": "dist/vee-validate.esm.js",
  "unpkg": "dist/vee-validate.js",
  "main": "dist/vee-validate.js",
  "typings": "dist/types/src/index.d.ts",
  "homepage": "https://baianat.github.io/vee-validate/",
  "repository": "https://github.com/baianat/vee-validate",
  "scripts": {
    "test": "jest",
    "lint": "eslint . '**/*.{js,jsx,ts,tsx}' --fix",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./scripts/deploy.sh",
    "build:locale": "node scripts/locale.js",
    "build:main": "node scripts/main.js",
    "ts:defs": "tsc --emitDeclarationOnly",
    "build": "npm run build:main && npm run ts:defs && npm run build:locale",
    "cover": "jest --coverage"
  },
  "keywords": [
    "VueJS",
    "Vue",
    "validation",
    "validator",
    "inputs",
    "form"
  ],
  "files": [
    "dist/*.js",
    "dist/locale/*.js",
    "dist/types/**/*.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@commitlint/cli": "^8.0.0",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "filesize": "^4.1.2",
    "flush-promises": "^1.0.2",
    "gzip-size": "^5.1.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "mkdirp": "^0.5.1",
    "polish-plurals": "^1.1.0",
    "prettier": "^1.18.2",
    "raf-stub": "^3.0.0",
    "rollup": "^1.16.7",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-jest": "^24.0.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3",
    "uglify-js": "^3.6.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.0.2",
    "vuepress-plugin-demo-code": "^0.3.5"
  },
  "eslintIgnore": [
    "locale",
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ],
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": "^2.5.18"
  }
}
