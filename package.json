{
  "name": "vee-validate",
  "version": "3.0.0-beta.0",
  "description": "Template Based Validation Framework for Vue.js",
  "author": "Abdelrahman Awad <logaretm1@gmail.com>",
  "license": "MIT",
  "module": "dist/vee-validate.esm.js",
  "unpkg": "dist/vee-validate.js",
  "main": "dist/vee-validate.js",
  "typings": "dist/types/src/index.d.ts",
  "homepage": "https://baianat.github.io/vee-validate/",
  "repository": "https://github.com/baianat/vee-validate",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "flow": "flow",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./scripts/deploy.sh",
    "build:locale": "node scripts/locale.js",
    "build:main": "node scripts/main.js",
    "ts:defs": "tsc --emitDeclarationOnly",
    "build": "npm run build:main && npm run ts:defs && npm run build:locale",
    "cover": "jest --coverage"
  },
  "keywords": [
    "VueJS",
    "Vue",
    "validation",
    "validator",
    "inputs",
    "form"
  ],
  "files": [
    "dist/*.js",
    "dist/locale/*.js",
    "dist/types/**/*.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@commitlint/cli": "^7.5.2",
    "@types/jest": "^24.0.11",
    "@types/validator": "^10.11.0",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "filesize": "^4.1.2",
    "flush-promises": "^1.0.2",
    "gzip-size": "^5.1.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "mkdirp": "^0.5.1",
    "polish-plurals": "^1.1.0",
    "prettier": "^1.17.0",
    "rollup": "^1.10.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.3",
    "uglify-js": "^3.5.4",
    "validator": "^10.8.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.9.0",
    "vue-jest": "^3.0.4",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^0.14.11"
  },
  "eslintIgnore": [
    "locale",
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ],
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ]
  },
  "dependencies": {}
}
